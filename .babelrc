{
  "presets": ["es2015", "react"],
  "plugins": [
    ["babel-plugin-transform-builtin-extend", {
      "globals": ["Error"]
    }]
  ],
  "env": {
    "testing": {
      "plugins": [
        "rewire",
        [
          "module-alias",
          [
            {
              "src": "./src",
              "expose": "src"
            }
          ]
        ],
        [
          "webpack-alias",
          {
            "config": "./webpack/webpack.config.js"
          }
        ]
      ]
    }
  }
}
